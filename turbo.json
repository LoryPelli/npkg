{
    "$schema": "https://turborepo.com/schema.json",
    "ui": "tui",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "cache": false
        },
        "deploy": {
            "dependsOn": ["^deploy"],
            "cache": false
        },
        "dev": {
            "cache": false,
            "persistent": true
        }
    }
}
