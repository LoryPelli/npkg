{
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": false,
        "moduleResolution": "NodeNext",
        "module": "NodeNext",
        "target": "ESNext",
        "verbatimModuleSyntax": true,
        "isolatedModules": true,
        "lib": ["esnext", "DOM", "DOM.Iterable"]
    }
}
